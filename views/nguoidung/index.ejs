      <section id="sliderSection">
        <div class="row">
          <div class="col-lg-8 col-md-8 col-sm-8">
            <div class="slick_slider">
              <%for(var i in arr){ %>
                <div class="single_iteam">
                  <a href="/content/<%-arr[i]._id%>">
                    <img src="<%=arr[i].Hinh%>" alt="">
                  </a>
                  <div class="slider_article">
                    <h2>
                      <a class="slider_tittle" href="/content/<%-arr[i]._id%>">
                        <%-arr[i].TieuDe%>
                      </a>
                    </h2>
                    <p><%-arr[i].NoiDung_text.slice(0,100)%>.......</p>
                  </div>
                </div>
                <%};%>
            </div>
          </div>
          <div class="col-lg-4 col-md-4 col-sm-4">
            <div class="latest_post">
              <h2>
                <span>Bài đăng mới nhất</span>
              </h2>
              <div class="latest_post_container">
                <div id="prev-button">
                  <i class="fa fa-chevron-up"></i>
                </div>
                <ul class="latest_postnav">
                  <li>
                    <%for(var i in arr){ %>
                      <%if(i>5 && i<11){%>
                      <div class="media">
                        <a href="/content/<%-arr[i]._id%>" class="media-left">
                          <img alt="" src="<%=arr[i].Hinh%>"> </a>
                        <div class="media-body">
                          <a href="/content/<%-arr[i]._id%>" class="catg_title">
                            <%-arr[i].TieuDe%>
                            <br>
                            <h6><%-arr[i].ThoiGian.format('dddd ,yyyy-mm-dd HH:MM:ss')%></h6>
                          </a>
                        </div>
                      </div>
                      <%}};%>
                  </li>
                </ul>
                <div id="next-button">
                  <i class="fa  fa-chevron-down"></i>
                </div>
              </div>
            </div>
          </div>
        </div>
