            <div class="latest_newsarea">
              <span>Tin mới nhất</span>
              <ul id="ticker01" class="news_sticker">
                <%for(var i in arr){ %>
                  <li>
                    <a href="/content/<%-arr[i]._id%>">
                      <img src="<%=arr[i].Hinh%>" alt="">
                      <%-arr[i].TieuDe%>
                    </a>
                  </li>
                  <%};%>
              </ul>
              <div class="social_area">
                <ul class="social_nav">
                  <li class="facebook">
                    <a href="#"></a>
                  </li>
